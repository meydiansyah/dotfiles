# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"

#--- Editor --------------------------------------------------------------------------

alias eal='n ~/dotfiles/.zsh_aliases'
alias exp='n ~/dotfiles/.zsh_export'
alias n=nvim

#--- Source --------------------------------------------------------------------------

alias so='source ~/.zshrc'
alias sot='tmux source ~/.tmux.conf'

#--- Project directore ---------------------------------------------------------------

alias inf='~/AndroidStudioProjects/project/2021_12/infosiss_trucking && n'
alias fp='~/AndroidStudioProjects/project/ && clear'

#--- Github command ------------------------------------------------------------------

alias gs='git status'
alias gpm='git push -u origin master'
alias gpod='git push develop'
alias gcod='git checkout develop'
alias gcom='git checkout master'
alias gmd='git checkout develop'
alias gmm='git checkout master'

#--- Laravel command ----------------------------------------------------------------

alias pa='php artisan'
alias pas='php artisan serve'
alias pamc='php artisan make:component'
alias pamct='php artisan make:controller'
alias paml='php artisan make:livewire'
alias pams='php artisan make:seed'
alias pamm='php artisan make:model '
alias lp='~/Documents/dev/php/laravel/ && clear'

alias sail='[ -f sail ] && bash sail || bash vendor/bin/sail'

#--- General command ----------------------------------------------------------------

alias ll='ls -lah'
alias x='exit'
alias tm='tmate'
alias c='clear'
alias rmd='sudo rm -r '

# tmux
alias t='tmux'
alias tn='tmux new -s'
alias tks='tmux kill-session -t'
alias tas='tmux attach-session -t'

# open scrcpy flutter develop
alias s='scrcpy -Sw --window-x 1000' 

# mysql
alias mysql=/usr/local/mysql/bin/mysql
alias mysqladmin=/usr/local/mysql/bin/mysqladmin
alias myloc="mysql -u root --password='password'"
